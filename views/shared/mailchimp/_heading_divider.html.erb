<!-- HEADER -->
<div style="padding:0;margin:0 0 15px 0;">
  <table>
    <tbody>
      <tr>
        <td style="text-align:left;font-size:15px;line-height:19px;;color:#000000;width:49%;vertical-align:middle">
          <hr style="margin:0;padding:0;border:none;border-bottom:3px solid #e5e5e5">
        </td>
        <th style="width:2%;padding:0 10px;white-space:nowrap;vertical-align:middle">
          <h1 style="font-size:28px;line-height:normal;text-align:center;margin:0 auto">
            <span style="font-size:28px;font-weight:bold;color:#000000;letter-spacing:normal;"><%= primary_text %></span><br>
            <span style="font-weight:normal;font-size:16px;color:#000000;letter-spacing:normal;"><%= secondary_text %></span>
          </h1>
        </th>
        <td style="font-size:15px;line-height:19px;width:49%;vertical-align:middle">
          <hr style="margin:0;padding:0;border:none;border-bottom:3px solid #e5e5e5">
        </td>
      </tr>
    </tbody>
  </table>
</div>
