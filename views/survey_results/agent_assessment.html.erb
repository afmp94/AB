      <div class="three wide column">
        <div class="ui inverted link list">
          <%= link_to("Homepage", "https://www.agentbright.com/", class: "item") %>
        </div>
      </div>
      <div class="three wide column">
        <div class="ui inverted link list">
          <%= link_to(
            "Sign in",
            "https://app.agentbright.com/",
            class: "item"
          ) %>
          <%= link_to(
            "Terms of Service",
            "https://www.agentbright.com/terms-of-service",
            target: :_blank,
            class: "item"
          ) %>
          <%= link_to(
            "Privacy Policy",
