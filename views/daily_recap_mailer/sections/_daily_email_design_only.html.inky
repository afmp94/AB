          <span style="color:#aa9c84;letter-spacing:normal;">
            No Client Activity Yesterday
          </span>
        </h2>
      <%# end %>
      <spacer size="6"></spacer>
      <%# @leads_for_client_activities.each do |lead| %>
        <!-- CLIENT -->
        <row>
          <columns small="1" large="1">
            <%# if lead.contact&.display_image&.file&.url.present? %>
              <img style="border-radius: 100px;-webkit-border-radius: 100px; -moz-border-radius: 100px;" src="http://semantic-ui.com/images/avatar/small/jenny.jpg">
            <%# end %>
          </columns>
          <columns small ="6" large="6">
            <p style="margin: 1em 0;padding: 0;-ms-text-size-adjust: 100%;-webkit-text-size-adjust: 100%;color: #606060;font-family: Arial, 'Helvetica Neue', Helvetica, sans-serif;font-size: 14px;line-height: 150%;text-align: left;">
              <span style="font-weight:normal;font-size:12px;line-height:16px;padding:0 7px;display:inline-block;border-radius:12px">
                <%#= link_to(
                  lead.name,
                  lead_url(lead),
                  target: "_blank",
                  rel: "noopener noreferrer",
                  style: "color:black;text-decoration:none;font-weight:bold;letter-spacing:normal;"
                ) %>
                Cindy Davis
            </span>
          </p>
          </columns>
          <columns small="3" large="3">
            <span style="font-weight:normal;font-size:12px;line-height:16px;padding:0 7px;display:inline-block;border-radius:12px">
              <row>
                cindy@gmail.com
                <%#= display_primary_email_address(lead.contact) %>
              </row>
              <row>
                758-123-4532
                <%#= display_primary_phone_number(lead.contact) %>
              </row>
            </span>
          </columns>
          <columns small="2" large="2">
            <button href="<%#= lead_url(lead) %>" target="_blank" class="rounded small-expanded secondary button-size: tiny">Update</button>
          </columns>
        </row>
        <%# if lead.tasks_created_or_completed_since_yesterday.present? %>
          <!-- TO-DOs -->
            <h3 style="font-size:14px;font-weight:bold;line-height:20px;margin:4px 0"><span style="color:#aa9c84;letter-spacing:normal;">2 tasks added and 1 task completed<%#= display_number_of_tasks_create_and_completed_since_yesterday(lead) %></span></h3>
          <!-- LIST -->
            <%# lead.tasks_created_or_completed_since_yesterday.not_completed.each do |task| %>
              <!-- TO-DO -->
              <row>
                <columns small="1" large="1" ></columns>
                <columns small="1" large="1" style= style= "font-weight:normal;text-align:right;font-size:14px; line-height:80%">❏</columns>

                <columns small="10" large="10" style="font-size:15px;line-height:19px;color:#000000;padding:0;border-collapse:collapse;border-spacing:0;padding-top:2px;padding-bottom:2px;vertical-align:top;width:99%; line-height:80%">
                  <%#= link_to(
                    task_url(task),
                    target: "_blank",
                    rel: "noopener noreferrer",
                    style: "color:black;text-decoration:none"
                  ) do %>
                    <span>Call health department<%#= task.subject %></span>
                    <span style="font-weight:normal;font-size:12px;line-height:80%;padding:0 7px;color:#666;display:inline-block;border-radius:12px">
                      Date due:   2/4/18<%#= task_pill_contents(task) %>
                    </span>
                  <%# end %>
                </columns>
              </row>
            <%# end %>

            <%# lead.tasks_created_or_completed_since_yesterday.completed.each do |task| %>
              <!-- TO-DO -->
                <row>
                  <columns small="1" large="1" ></columns>
                  <columns small="1" large="1" style= "font-weight:normal;text-align:right;font-size:14px">✔</columns>
                  <columns  small="10" large="10" style= "font-weight:normal;text-align:leftt;font-size:14px">
                    <%#= link_to(
                      task_url(task),
                      target: "_blank",
                      rel: "noopener noreferrer",
                      style: "color:black;text-decoration:none"
                    ) do %>
                      <span>Arrange Photos for house<%#= task.subject %></span>
                      <span style="font-weight:normal;font-size:12px;line-height:15px;color:grey">
                        (Completed by Jane Jones<%#= task.completed_by_name %>)
                      </span>
                    <%# end %>
                  </columns >
                </row>
                <row>
                  <columns small="1" large="1" ></columns>
                  <columns small="1" large="1" style= "font-weight:normal;text-align:right;font-size:14px">✔</columns>
                  <columns  small="10" large="10" style= "font-weight:normal;text-align:leftt;font-size:14px">
                    <%#= link_to(
                      task_url(task),
                      target: "_blank",
                      rel: "noopener noreferrer",
                      style: "color:black;text-decoration:none"
                    ) do %>
                      <span>Schedule Open House<%#= task.subject %></span>
                      <span style="font-weight:normal;font-size:12px;line-height:15px;color:grey">
                        (Completed by Jane Jones<%#= task.completed_by_name %>)
                      </span>
                    <%# end %>
                  </columns >
                </row>
            <%# end %>
          <%# end %>

          <%# if lead.comments_created_since_yesterday.present? %>
            <!-- DISCUSSIONS -->
              <!-- HEADING -->
            <h3 style="font-size:14px;font-weight:bold;line-height:20px;margin:4px 0"><span style="color:#aa9c84;letter-spacing:normal;">1 comment<%#= pluralize(lead.number_of_comments_created_since_yesterday, "comment was", plural: "comments were") %> made:</span></h3>
            <%# lead.comments_created_since_yesterday.each do |comment| %>
              <!-- DISCUSSION -->
              <%#= link_to(
                lead_url(lead),
                target: "_blank",
                rel: "noopener noreferrer",
                style: "color:black;text-decoration:none"
              ) do %>
              <!-- TITLE -->
                <p style="font-size:15px;line-height:19px;color:#000000;margin:0 0 19px;font-weight:bold;margin-bottom:0!important">
                  John
                  <%#= comment.user.first_name %> wrote at <%#= just_time(comment.updated_at) %>:
                </p>
                <!-- META DATA -->
                <p style="margin:6px 0 19px 20px;margin-bottom:0;font-size:14px;color:grey;line-height:18px;font-style:italic;">
                  Be sure to check with Beth on her plans for creative staging plans
                  <%#= comment.content %>
                </p>
              <%# end %>
            <%# end %>
          <%# end %>
        <hr class="style5">
      <%# end %>
      View All Clients (this is a hyperlink)
    </columns>
  </row>
<%# else %>
  <spacer size="6"></spacer>
